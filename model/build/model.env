
export EXNUC=/opt/ingr/exnucdp
export EXTARG=/opt/ingr/exnucdp

export GRNUC=/opt/ingr/grnucdp

export MODEL=$ISDP_SOURCE/model
export HSURF=$ISDP_SOURCE/model
export MDTARG=$ISDP_TARGET/model

export XINC=/usr/openwin/include
export OM=$EXNUC
export FORMS=/opt/ingr/xformsdp/xfi
export UMS=/opt/ingr/ums

export PATH=$PATHX:/opt/ingr/bin:$GRNUC/bin:$EXNUC/bin
export PATH=$PATH:$ISDP_SOURCE/grnuc/build
export PATH=$PATH:$ISDP_SOURCE/model/build

# ISL is Solaris x86
# Might need the CLIPPER set
export CLIPPER=n
export C400=n
export SUN=n
export SUN5=n
export SUNZX=n
export SGI=n
export ISL=y

export MASBLDLOG=model_build.log
export ERRNOTI=off
export GRCOMPILE_SWITCH=4

export WORKDIR=$ISDP_SOURCE/model
export SOURCEDIR=$ISDP_SOURCE/model

export COBCPPOPT="-I${EXNUC}/include -I${GRNUC}/include -I${MODEL}/include -I${BS}/include -I/opt/ingr/ums/include -I/opt/ingr/xformsdp/xfi/include"
