CC = cc

$(MODEL)/bin/graph: graph.o graphics.o
	@echo
	cc  -o $(MODEL)/bin/graph  graph.o graphics.o -lX11

.c.o:
	@echo "    $*.c -> $@"
	@if test "${DEBUG}" != "yes"; \
	then ${CC} -c  ${MKDOPT} $<; \
	else ${CC} -c -g -DDEBUG=1 ${MKDOPT} $<; fi
	@if test "${DEBUG}" = "yes"; \
	then echo "    debug version of $*.c compiled"; fi

