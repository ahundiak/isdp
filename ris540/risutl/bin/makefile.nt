risbatch: $(BINDIR)\risbatch.exe
$(BINDIR)\risbatch.exe: $(RISBATCH_OBJS) $(RISBATCH_RESOURCES) $(RISBATCH_LIBS)
	@echo.
	@echo	RISBATCH_OBJS RISBATCH_RESOURCES RISBATCH_LIBS   -to-   $@
	@echo 	Linking with $(link) $(linkdebug) $(conflags)
	@$(link) $(linkdebug) $(conflags) -out:$(@) @<<
$(RISBATCH_OBJS)
$(RISBATCH_RESOURCES)
$(RISBATCH_LIBS)
$(conlibsdll)
<<

