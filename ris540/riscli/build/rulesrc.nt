$(RC_OBJECTS):
	@echo.
	@echo 	$(@:.obj=.rc)   -to-   $(OBJDIR)\$@
	@$(BECAUSE) @<<
$?
<<
	@echo 	Preprocessing with $(RISCPP)
	@$(RISCPP) $(IPATH) -r $(SRC)\$(@:.obj=.rc) 
	@echo 	Compiling with $(cc) $(cdebug) $(cflags) $(cvarsdll)
	@$(cc) $(cdebug) $(IPATH) $(cflags) $(cvarsdll) $(@:.obj=.c)
	@del $(@:.obj=.c)
