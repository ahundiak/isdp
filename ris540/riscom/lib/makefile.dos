!include $(RISDEVDIR)\ristools\include\makeenv.dos
!include $(RISDEVDIR)\riscom\build\defines.dos

fast:   clean $(LIBDIR)\riscom.lib

clean:  
	@del $(LIBDIR)\riscom.lib

$(LIBDIR)\riscom.lib: $(RIS_COMMON_ARCHIVE_OBJS)
	@echo.
	@echo  RIS_COMMON_ARCHIVE_OBJS  -to-  $@
	@echo -nobanner -twocase -create > libfiles.lbc
	@dir/b/s $(RIS_COMMON_ARCHIVE_OBJS) >> libfiles.lbc
	$(DOSLIB) $(LIBDIR)\riscom.LIB @libfiles
	@del libfiles.lbc
