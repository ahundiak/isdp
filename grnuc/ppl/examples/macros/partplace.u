/****************************************************************************/
/*								            */
/*  Program:  partplace.u						    */
/*  Macro(s):	co$part_place						    */
/*  Defined in:  partmacros.h         				            */
/*  Comments: Requires a cell library with a cell active.  Uses the cell    */
/*            to place a part.						    */
/****************************************************************************/

#include	"ciminimum.h"
#include	"cimacros.h"
#include	"cieveryone.h"
#include	"grio.h"
#include	"partdef.h"
#include	"part.h"
#include	"partmacros.h"
#include	"msdef.h"
extern int	COpart_place_func();

main()
{
IGRlong	msg;
struct GRmd_env	md_env;
struct	GRevent	origin;
struct	GRid	part_id;
int	sizepd;

sizepd = sizeof (struct GRid);

ci$get_module_info(md_env = &md_env);

ci$getevent(
	prompt	= "// Specify Part Origin //",
	event	= &origin);

co$part_place(
	msg	= &msg,
	mod_env = &md_env,
	origin	= &origin,
	part	= &part_id,
	num 	= &sizepd);

if (msg == CO_E_NO_CELL)
	write("No active cell found\n");
else
	if (msg != MSSUCC)
		write("Part place failed\n");
	else
		write("part_id.objid = ",part_id.objid,"\n");
}	/* End Main */
