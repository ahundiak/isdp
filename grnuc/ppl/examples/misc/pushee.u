/************************************************************
 * FILE:  pushee.u
 *
 * PURPOSE:  This PPL command file is used by pusher.u.
 *
 * DESCRIPTION: This command file contains the func1()
 *    function, which pusher.u invokes.
 ************************************************************/

#include "ciminimum.h"


func1( arg1, arg2, arg3, arg4)
/************************************************************
 * FUNCTION: func1()
 *
 * DESCRIPTION: This function writes its arguments to the
 *    CLIX window.
 ************************************************************/
int    arg1, arg2;
double arg3;
char   arg4[20];
{
/************************************************************
  Write the arguments to the CLIX window.
 ************************************************************/
  write( "Output from the func1() function:\n\n",
         "   arg1 = ", arg1, "\n",
         "   arg2 = ", arg2, "\n",
         "   arg3 = ", arg3, "\n",
         "   arg4 = ", arg4, "\n"  );
}


main( )
{
/* The main() function is not used. */
}

