/*---
  asap2_toggle.u
---*/

#include "ciminimum.h"
#include "cimacros.h"
char cmdname[40];
extern int ASAPII_status;

main ()
{
  ASAPII_status = ASAPII_status ^ 1;
  if (ASAPII_status & 1)
    strcpy (cmdname, "using asapII");
  else
    strcpy (cmdname, "not using asapII");
  message (cmdname);
  status (cmdname);
  exit;
}
