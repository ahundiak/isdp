#
#   3-98	KDD	Added Cable directory tree.
#

File="IROUTEsrc"
CurDir=$PWD

echo "		Saving I/ROUTE sources in $CurDir/$File "
echo
echo
echo " I/ROUTE located in : $ROUTE "
echo
echo

cd $ROUTE

find 	fixes \
	include \
	vr* \
	vc* \
	config/english \
	! -name *.o -print |\
	grep -v makefile |\
	grep -v version.info |\
	cpio -ov |\
	compress > $CurDir/$File
