/***************************************************************************
 * I/STRUCT
 *
 * File:        api/APIwldASCII.u
 *
 * Description:
 *
 *  This ppl informs about the joint input information, stored in ASCII files,
 *		using the macro vs$getWeldCodeASCII for weld codes and
 *			  macro vs$getWeldProcASCII for weld processes
 *		      
 *
 * Dependencies:
 *
 * History:
 *      MM/DD/YY        AUTHOR          DESCRIPTION
 *                      india           creation date
 *
 ***************************************************************************/

#include "ciminimum.h"
#include "cimacros.h"
#include "cieveryone.h"
#include "vsjntmacros.h"
#include "vsdbgmacros.h"

extern	long	VSgetWeldCodeASCII();
extern	long	VSgetWeldCodeASCII();

extern		VSfreeList();
extern		printf();


main() 
{
	long            sts, msg;
	int		count, i;
	char 		**codeList;
	char 		**procList;

	*codeList = NULL ;
	*procList = NULL ;

	sts =
	vs$getWeldCodeASCII( 	msg      = &msg,
				count    = &codeCnt,
				list     = &codeList ) ;

        __CheckRC( sts, msg, "vs$getWeldCodes", wrapup ) ;

	printf("List Of Weld Codes from ASCII file \n");
	printf("-----------------------------------\n\n");
	for( i=0 ; i<codeCnt ; i=i+1 )	printf("%s", codeList[i] );


	/*
	 * Get the welding Process Information.
	 */
	sts =
	vs$getWeldProcASCII( 	msg      = &msg,
				count    = &procCnt,
				list     = &procList ) ;
        __CheckRC( sts, msg, "vs$getWeldCodes", wrapup ) ;

	printf("List Of Weld Process from ASCII file \n");
	printf("-------------------------------------\n\n");

	for( i=0 ; i<procCnt ; i=i+1 )	printf("%s", procList[i] );

wrapup:

	VSfreeList( codeCnt, codeList );
	VSfreeList( procCnt, procList );

}


