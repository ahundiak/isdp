		$rc := vl-Print ( "elements",    20, -1 ) ;
		$rc := vl-Print ( "(degrees)",   32, -1 ) ;
		$rc := vl-Print ( "(degrees)",   47, -1 ) ;
		$rc := vl-Print ( "(degrees)",   62, -1 ) ;
		$rc := vl-Skip ();
	} # end of any-view
} # end of vl-rp-pb-Start
	
