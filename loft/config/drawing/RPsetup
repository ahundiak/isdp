		$rc := vl-Print ( "Number of",   20, -1 ) ;
		$rc := vl-Print ( " X angle",    32, -1 ) ;
		$rc := vl-Print ( " Y angle",    47, -1 ) ;
		$rc := vl-Print ( " Z angle",    62, -1 ) ;
		$rc := vl-Skip ();
