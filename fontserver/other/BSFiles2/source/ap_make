args "This is the 'make' file used to compile and link on the Apollo system."

# ccc is a conditional compile

# Use the following if you are compiling on a DN3000 or above

# ccc  sample "-systype bsd4.2 -cpu 3000"
# ccc  cache  "-systype bsd4.2 -cpu 3000"
# ccc  cfso_ac  "-systype bsd4.2 -cpu 3000"
# ccc  convert  "-systype bsd4.2 -cpu 3000"
# ccc  do_funct  "-systype bsd4.2 -cpu 3000"
# ccc  getrec  "-systype bsd4.2 -cpu 3000"
# ccc  rules  "-systype bsd4.2 -cpu 3000"
# ccc  speed  "-systype bsd4.2 -cpu 3000"


# Use the following if you are compiling on a machine below DN3000

ccc  sample "-systype bsd4.2"
ccc  cache  "-systype bsd4.2"
ccc  cfso_ac  "-systype bsd4.2"
ccc  convert  "-systype bsd4.2"
ccc  do_funct  "-systype bsd4.2"
ccc  getrec  "-systype bsd4.2"
ccc  rules  "-systype bsd4.2"
ccc  speed  "-systype bsd4.2"

bind -b sample - <<#
    sample.bin
    cache.bin
    cfso_ac.bin
    convert.bin
    do_funct.bin
    getrec.bin
    rules.bin
    speed.bin
    -end
#
args "'sample'"

