args "This is the 'make' file used to compile and link on the Apollo system."

cc  main  -systype bsd4.2
cc  alloc  -systype bsd4.2
cc  apollo  -systype bsd4.2
cc  bitmap  -systype bsd4.2
cc  bmap  -systype bsd4.2
cc  cache  -systype bsd4.2
cc  cfont_ac  -systype bsd4.2
cc  convert  -systype bsd4.2
cc  do_funct  -systype bsd4.2
cc  getrec  -systype bsd4.2
cc  getrec_a  -systype bsd4.2
cc  kern  -systype bsd4.2
cc  reader  -systype bsd4.2
cc  rules  -systype bsd4.2
cc  splitarc  -systype bsd4.2
cc  util  -systype bsd4.2
bind -map -sec -xref -b make_bmap - <<#
    main.bin
    alloc.bin
    apollo.bin
    bitmap.bin
    bmap.bin
    cache.bin
    cfont_ac.bin
    convert.bin
    do_funct.bin
    getrec.bin
    getrec_a.bin
    kern.bin
    reader.bin
    rules.bin
    splitarc.bin
    util.bin
    -end
    -glo
