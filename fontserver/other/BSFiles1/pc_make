alloc.obj: alloc.c alloc.h stdef.h
    cl -c /AH alloc.c

bitmap.obj: bitmap.c stdef.h
    cl -c /AH bitmap.c 

bmap.obj: bmap.c stdef.h bmap.h
    cl -c /AH bmap.c

cache.obj: cache.c stdef.h comp2_3.h mkbm.h
    cl -c /AH cache.c 

cfont_ac.obj: cfont_ac.c stdef.h comp2_3.h cfont_ac.h
    cl -c /AH cfont_ac.c 

convert.obj: convert.c stdef.h comp2_3.h mkbm.h polyfill.h
    cl -c /AH convert.c 

do_funct.obj: do_funct.c stdef.h
    cl -c /AH do_funct.c 

getrec.obj: getrec.c stdef.h
    cl -c /AH getrec.c 

ibm_pc.obj: ibm_pc.c stdef.h
    cl -c /AH ibm_pc.c 

kern.obj: kern.c comp2_3.h mkbm.h stdef.h bmap.h
    cl -c /AH kern.c 

main.obj: main.c stdef.h comp2_3.h mkbm.h polyfill.h
    cl -c /AH main.c

reader.obj: reader.c stdef.h comp2_3.h mkbm.h math.h
    cl -c /AH reader.c 

rules.obj: rules.c comp2_3.h mkbm.h stdef.h
    cl -c /AH rules.c 

splitarc.obj: splitarc.c comp2_3.h mkbm.h stdef.h polyfill.h
    cl -c /AH splitarc.c 

util.obj: util.c stdef.h
    cl -c /AH util.c 

makebmap.exe: main.obj alloc.obj bitmap.obj bmap.obj cache.obj cfont_ac.obj convert.obj do_funct.obj getrec.obj ibm_pc.obj kern.obj reader.obj rules.obj splitarc.obj util.obj
    link /stack:8192 @pc_make.lnk
