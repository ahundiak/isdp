/bin/cp ./VSdrwgrammar.h /usr/tmp
${GRNUC}/bin/grmake -f makeY
if /bin/diff ./VSdrwgrammar.h /usr/tmp > /dev/null ; then
	echo "\tVSdrwgrammar.h not changed"
	/bin/touch -c 01011200 VSdrwgrammar.h
fi
/bin/rm /usr/tmp/VSdrwgrammar.h

${GRNUC}/bin/grmake -f makeL
if [ VSdrwgrammar.h -nt ./mkfile ] ; then
	grmakemake vsdrawing.m mkfile
fi
if [ vsdrawing.m -nt ./mkfile ] ; then
	grmakemake vsdrawing.m mkfile
fi
${GRNUC}/bin/grmake -rk OPPOPT=-k -f mkfile

/bin/ls *.c > /dev/null 2>&1
RC=$?
if [ $RC -eq 0  ] ; then 
	for i in *.c ; do
		echo "\t ${i} -> ${i%c}o"
		${COMPILER} ${COMPOPT} ${MKDOPT} -I$EXNUC/include -c ${i}
		rm ${i}
	done
	${GRNUC}/bin/grmake -r -f mkfile
fi

