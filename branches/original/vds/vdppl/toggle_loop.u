// GLOBAL VARIABLE used    in: $VDS/emsfixes/grnuc/GMigraph.C
//                 defined in: $VDS/emsfixes/bs/VDdbg_glob.c

extern int toggle_GMloop;

main()
{
  if ( toggle_GMloop )
  {
    toggle_GMloop = 0;
    status ("toggle_loop: Execute GMLoop");
  }
  else
  {
    toggle_GMloop = 1;
    status ("toggle_loop: Do NOT Execute GMLoop");
  }
}
