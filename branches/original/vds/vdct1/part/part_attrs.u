#include "VDppl.h"
#include "VDct1Part.h"

main()
{
  TVDct1PartInfo part;
  TVDct1_part_fld_part_id part_id;
  
  printf("------- VDct1ReadPartAttrsTable --------\n");

  VDct1ReadPartId("000010721",part_id);

  printf("Part ID %s\n",part_id);
  
  VDct1ReadPartAttrsTable(part_id,&part);

  printf("Part %s %s %s %s\n",
	 part.part_num,
	 part.unit_of_measure,
	 part.material,
	 part.noun_name);
  
}
