#include "stdio.h"
#include "ciminimum.h"
#include "cimacros.h"

#include "igrdp.h"

#include "go.h"
#include "grmacros.h"

extern GRsymbology_options();

main()
{	
	struct GRsymbology symbology;
	struct GRid	object;
	IGRlong		msg;

   	ci$locate( obj 	 = &object.objid,
		   osnum = &object.osnum,
		   prompt= "Identify object");
  
	gr$get_symbology( object_id = &object, symb = &symbology );

	printf("color = %d\n",symbology.display_attr.color);
	printf("weight = %d\n",symbology.display_attr.weight);
	printf("style = %d\n",symbology.display_attr.style);
	printf("level = %d\n",symbology.level);

}
