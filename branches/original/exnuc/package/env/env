export DELloadp=555

export IDclass="Design o Engineering o Manufacturing"
export IDdate=$PRODUCT_DATE
export IDname=$PRODUCT_NAME
export IDversion=$PRODUCT_VERSION

export PDkey=Exnuc

OSVER=$(uname -r | cut -c1)
case `uname -s` in
  CLIX)
    DELdirDIR="ip32/" ;;
  SunOS)
    case $OSVER in
    4) DELdirDIR="ip32/" ;;
    5) DELdirDIR="" ;;
    esac
   ;;
  IRIX)
    case $OSVER in
    4) DELdirDIR="ip32/" ;;
    5) DELdirDIR="" ;;
    esac
   ;;
esac

case $PRODUCT_NAME
in
EXNUC)
	export IDnumber='SN01094AA-0304A'
	export DELdir=${DELdirDIR}$(echo $PRODUCT_NAME | tr "[A-Z]" "[a-z]")
	export IDtitle="Executive Nucleus Runtime System"
	export PRODUCT_DIR=${PRODUCT_DIR:-/usr2/exnuc34/Pex}
	export CPUTYPE_LIST="i5"
#	export CPUTYPE_LIST="c1 c4 s5 s6 i5 m5"
	export VARIANT=rt
	;;

EXNUCDP)
	export IDnumber='SN01018AA-0304A'
	export DELdir=${DELdirDIR}$(echo $PRODUCT_NAME | tr "[A-Z]" "[a-z]")
	export IDtitle="Executive Nucleus Development Platform"
	export PRODUCT_DIR=${PRODUCT_DIR:-/usr2/exnuc34/Pexdp}
	export CPUTYPE_LIST="i5"
#	export CPUTYPE_LIST="c1 c4 s5 s6 i5 m5"
	export VARIANT=dp
	;;

EXNUCX)
	export DELdir=${DELdirDIR}$(echo $PRODUCT_NAME | tr "[A-Z]" "[a-z]")
	export IDnumber='SMAM166AA-0304A'
	export IDtitle="Executive Nucleus X Runtime System"
	export PRODUCT_DIR=${PRODUCT_DIR:-/usr2/exnuc34/Pexx}
	export CPUTYPE_LIST="c1 c4"
	export VARIANT=rt
	;;

EXNUCXDP)
	export IDnumber='SMAM167AA-0304A'
	export DELdir=${DELdirDIR}$(echo $PRODUCT_NAME | tr "[A-Z]" "[a-z]")
	export IDtitle="Executive Nucleus X Development Platform"
	export PRODUCT_DIR=${PRODUCT_DIR:-/usr2/exnuc34/Pexxdp}
	export CPUTYPE_LIST="c1 c4"
	export VARIANT=dp
	;;

*)
	echo "$ARGV0: unknown PRODUCT_NAME: $PRODUCT_NAME" >&2
	exit 2
	;;
esac
