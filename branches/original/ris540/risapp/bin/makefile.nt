!include $(RISDEVDIR)\ristools\include\makeenv.nt
!include $(RISDEVDIR)\risapp\build\defines.nt

IPATH=-I$(RISDEVDIR)\riscom\internal\
	  -I$(RISDEVDIR)\risapp\internal

fast: $(BINDIR)\riscpp.exe

$(BINDIR)\riscpp.exe: $(RISCPP_OBJS) $(RISCPP_RESOURCES) $(RISCPP_LIBS)
	@echo.
	@echo 	RISCPP_OBJS RISCPP_RESOURCES RISCPP_LIBS  -to-   $@
	@echo 	Linking with $(link) $(conflags) $(conlibsdll)
	@$(link) $(linkdebug) $(conflags) -out:$@ $(RISCPP_OBJS) $(RISCPP_RESOURCES) $(RISCPP_LIBS) $(conlibsdll) "$(SETUPSDK_DIR)\csource\cingr.lib"
