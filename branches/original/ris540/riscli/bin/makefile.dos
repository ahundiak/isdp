!include $(RISDEVDIR)/riscli/build/defines.dos

all : $(BINDIR)\remove.exe

$(BINDIR)\remove.exp : $(REMOVE_OBJS)
	@echo.
	@echo 	$$REMOVE_OBJS    -to-    $@
	@dir/b/s $(REMOVE_OBJS) > binfiles.lbc
	@$(LINK) @<<
$(LFLAGS)
@binfiles.lbc
-exe $*.exp
<<
	@if exist binfiles.lbc del binfiles.lbc

$(BINDIR)\remove.exe : $(BINDIR)\remove.exp
	@echo. 
	@echo 	$(BINDIR)\remove.exp    -to-    $@
	@$(BIND) $(RUN386B) $*
