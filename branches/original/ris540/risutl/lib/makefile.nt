i9risfm6: $(LIBDIR)\i9risfm6.dll
$(LIBDIR)\i9risfm6.dll: risforms.def $(RIS_FORMS_LIB_OBJS) $(RIS_FORMS_LIB_RESOURCES) $(RIS_FORMS_LIB_LIBS) 
	@echo.
	@echo 	RIS_FORMS_LIB_OBJS RIS_FORMS_LIB_RESOURCES  -to-   $@
	@if exist $(LIBDIR)\i9risfm.lib del $(LIBDIR)\i9risfm.lib
	@if exist $(LIBDIR)\i9risfm.exp del $(LIBDIR)\i9risfm.exp
    @$(implib) @<<
-machine:$(CPU)
-def:risforms.def
$(RIS_FORMS_LIB_OBJS)
$(RIS_FORMS_LIB_RESOURCES)
-out:$(LIBDIR)\i9risfm.lib
<<
	@$(link) @<<
$(linkdebug)
-base:0x60800000 
-dll
-entry:_CRT_INIT$(DLLENTRY)  
-out:$(LIBDIR)\i9risfm6.dll 
-ignore:505
$(LIBDIR)\i9risfm.exp $(guilibsdll) $(RIS_FORMS_LIB_OBJS) $(RIS_FORMS_LIB_RESOURCES) $(RIS_FORMS_LIB_LIBS)
<<

