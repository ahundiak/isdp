risuco: $(LIBDIR)/risuco.lib
$(LIBDIR)/risuco.lib: $(RIS_UCO_LIB_OBJS)
	@echo.
	@echo 	RIS_UCO_LIB_OBJS  -to-   $@
	@dir/b/s $(RIS_UCO_LIB_OBJS) > libfiles.txt
	@if exist $(LIBDIR)\risuco.lib del $(LIBDIR)\risuco.lib
	@lib /out:$(LIBDIR)\risuco.lib @libfiles.txt
	@if exist libfiles.txt del libfiles.txt

