
mkdir -p $SP/data/vds 2> /dev/null

cp std_pcmk.ddl $SP/data/vds
cp std_pcmk.rm  $SP/data/vds

cp bevel_info.ddl $SP/data/vds
cp bevel_info.rm  $SP/data/vds

cp tm_table.ddl $SP/data/vds
cp tm_table.ris $SP/data/vds
cp lpdpallets.txt $SP/data/vds
cp lpdcode.txt $SP/data/vds
cp tmtreeindex.txt $SP/data/vds

cp pid.ddl $SP/data/vds
cp pid.rm  $SP/data/vds

cp tm_items.ddl  $SP/data/vds
cp tm_items.rm   $SP/data/vds
cp tm_items.ris  $SP/data/vds
cp ops_isdp.txt  $SP/data/vds
cp tm_ioc.ddl    $SP/data/vds
cp tm_ioc.ris    $SP/data/vds
cp tm_ioc.rm     $SP/data/vds
cp tm_sp14upd.ddl $SP/data/vds
cp tm_sp14upd.ris $SP/data/vds
cp ElecDwgCodes.txt $SP/data/vds

cp blobs.ddl $SP/data/vds
cp blobs.rm  $SP/data/vds

# Sneak these in here
mkdir -p  $SP/bin 2> /dev/null

cp gzip   $SP/bin
chmod 775 $SP/bin/gzip

cp gunzip $SP/bin
chmod 775 $SP/bin/gunzip
