umask 2

PATH=.:$HOME/bin:/bin:/etc:/usr/bin:
CDPATH=.:/usr:/usr/ip32
EDITOR=vi
ENV=$HOME/.env
my_host=`uname -n`
PS1=\($my_host\)'$PWD\> '
export PATH PS1 CDPATH ENV EDITOR

rel_num=`/usr/bin/awk -F= '/IDversion/ { print $2 ; }' $HOME/ws_s/vds/product.def`
echo "\n"
echo "			Welcome to $my_host system !"
echo "  This login contains a release of I/VDS for developpers which is $rel_num"
echo "\n"

