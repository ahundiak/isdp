Interactive Query Test	(type ^d to end)

query>"#lex_test.in - lexical analyzer test input"
query>"#		run test with LEXDEBUG defined"
query>"#		correct output is defined in lex_test.out "
query>"#"
query>"# test for all tokens"
query>"select A (a = 1) & (a != 1) | ((a >= 2)) @ !!(a <= 2)"
	SELECT
	IDENT "A"
		(
	IDENT "a"
		= 	op value: 1
	IDENT "1"
	   VDquery: 	 class:   A	body: a 1 1	status: 1
		)
		&
		(
	IDENT "a"
		!=	op value: 2
	IDENT "1"
	   VDquery: 	 class:   A	body: a 2 1	status: 0
		)
		|
		(
		(
	IDENT "a"
		>=	op value: 3
	IDENT "2"
	   VDquery: 	 class:   A	body: a 3 2	status: 1
		)
		)
		@
		!
		!
		(
	IDENT "a"
		<=	op value: 4
	IDENT "2"
	   VDquery: 	 class:   A	body: a 4 2	status: 0
		)
  returns: 1

query>"select A - B - C (a > 1) & (a < 1) ; select A where a = 10"
	SELECT
	IDENT "A"
		-
	IDENT "B"
		-
	IDENT "C"
		(
	IDENT "a"
		> 	op value: 5
	IDENT "1"
	   VDquery: 	 class:   A - B - C	body: a 5 1	status: 1
		)
		&
		(
	IDENT "a"
		< 	op value: 6
	IDENT "1"
	   VDquery: 	 class:   A - B - C	body: a 6 1	status: 0
		)
		;
	SELECT
	IDENT "A"
	WHERE
	IDENT "a"
		= 	op value: 1
	IDENT "10"
	   VDquery: 	 class:   A	body: a 1 10	status: 1
  returns: 1

query>"#"
query>"# test identifier parsing"
query>"select A(a=1&a!=1|a>=2@!!a<=2)"
	SELECT
	IDENT "A"
		(
	IDENT "a"
		= 	op value: 1
	IDENT "1"
	   VDquery: 	 class:   A	body: a 1 1	status: 0
		&
	IDENT "a"
		!=	op value: 2
	IDENT "1"
	   VDquery: 	 class:   A	body: a 2 1	status: 1
		|
	IDENT "a"
		>=	op value: 3
	IDENT "2"
	   VDquery: 	 class:   A	body: a 3 2	status: 0
		@
		!
		!
	IDENT "a"
		<=	op value: 4
	IDENT "2"
	   VDquery: 	 class:   A	body: a 4 2	status: 1
		)
  returns: 1

query>"select A-B-C(a>1&a<1);select A where(a=10)"
	SELECT
	IDENT "A"
		-
	IDENT "B"
		-
	IDENT "C"
		(
	IDENT "a"
		> 	op value: 5
	IDENT "1"
	   VDquery: 	 class:   A - B - C	body: a 5 1	status: 0
		&
	IDENT "a"
		< 	op value: 6
	IDENT "1"
	   VDquery: 	 class:   A - B - C	body: a 6 1	status: 1
		)
		;
	SELECT
	IDENT "A"
	WHERE
		(
	IDENT "a"
		= 	op value: 1
	IDENT "10"
	   VDquery: 	 class:   A	body: a 1 10	status: 0
		)
  returns: 0

query>"#"
query>"# identifiers with special characters"
query>"select A_B,./?\'":]}[{_*^%$#~` where 1_B,./?\'":]}[{_*^%$#~`  = "_,./?\'":]}[{_*^%$#~`""
	SELECT
	IDENT "A_B,./?\'":]}[{_*^%$#~`"
	WHERE
	IDENT "1_B,./?\'":]}[{_*^%$#~`"
		= 	op value: 1
	IDENT ""_,./?\'":]}[{_*^%$#~`""
	   VDquery: 	 class:   A_B,./?\'":]}[{_*^%$#~`	body: 1_B,./?\'":]}[{_*^%$#~` 1 "_,./?\'":]}[{_*^%$#~`"	status: 1
  returns: 1

query>"select A where a = "1a""
	SELECT
	IDENT "A"
	WHERE
	IDENT "a"
		= 	op value: 1
	IDENT ""1a""
	   VDquery: 	 class:   A	body: a 1 "1a"	status: 0
  returns: 0

query>"select A where a = '1a'"
	SELECT
	IDENT "A"
	WHERE
	IDENT "a"
		= 	op value: 1
	IDENT "'1a'"
	   VDquery: 	 class:   A	body: a 1 '1a'	status: 1
  returns: 1

