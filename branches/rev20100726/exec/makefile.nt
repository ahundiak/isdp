objects:\
	directories \
	_LOCALE \
	_FONT_EXEC \
	_CMDSERVER \
	_EXEC \
	_EXECCO \
	_MODULE \
	_WFI \
	_IGEWINDOW \
	_SH_EXEC

directories:
	@if not exist $(TARGET)\exec\lib mkdir $(TARGET)\exec\lib
	@if not exist $(TARGET)\exec\locale mkdir $(TARGET)\exec\locale
	@if not exist $(TARGET)\exec\fontexec mkdir $(TARGET)\exec\fontexec
	@if not exist $(TARGET)\exec\cmdserver mkdir $(TARGET)\exec\cmdserver
	@if not exist $(TARGET)\exec\exec mkdir $(TARGET)\exec\exec
	@if not exist $(TARGET)\exec\execco mkdir $(TARGET)\exec\execco
	@if not exist $(TARGET)\exec\module mkdir $(TARGET)\exec\module
	@if not exist $(TARGET)\exec\wfi mkdir $(TARGET)\exec\wfi
	@if not exist $(TARGET)\exec\igewindow mkdir $(TARGET)\exec\igewindow
	@if not exist $(TARGET)\exec\shexec mkdir $(TARGET)\exec\shexec
	@if not exist $(TARGET)\exec\driver mkdir $(TARGET)\exec\driver
	@if not exist $(TARGET)\exec\jnl mkdir $(TARGET)\exec\jnl

binaries:\
	directories \
	_DRIVER \
	_JNL

_LOCALE: $(TARGET)\exec\locale\makefile
	@cd $(TARGET)\exec\locale
	@cd
	@$(MAKE)

$(TARGET)\exec\locale\makefile: $(EXNUC)\exec\locale\locale.m
	cd $(EXNUC)\exec\locale
	$(MAKEMAKE) locale.m $(TARGET)\exec\locale\makefile

