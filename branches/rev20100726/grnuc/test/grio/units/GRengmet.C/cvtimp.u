extern int GRcvt_imp_to_alias();

IGRlong msg;
IGRdouble area;
IGRshort num_alias;
IGRchar output_meas_list[52];
int status;

main()
{

write ("\nentering cvtimp.u\n");

msg = MSSUCC;
area = 18.0;
num_alias = 1;

    status = co$cvt_imp_to_alias(msg = &msg,
                                 unit_type = "UOM_AREA",
                                 osnum = 2,
                                 working_units = &area,
                                 num_alias = &num_alias,
				 inverse_scale_flag = GRIO_NO_SCALE,
                                 cvt_list = output_meas_list);

write ("after cvt_imp_to_alias call\n");
write ("status = ",status, "\n");
write ("msg = ", msg, "\n");
write ("MSSUCC = ", MSSUCC, "\n");
write ("MSFAIL = ", MSFAIL, "\n");
write ("value = ", output_meas_list, "\n");
write ("\n");

}
