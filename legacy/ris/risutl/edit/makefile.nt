BASE_NAME=edit
EDIT_LIBS=\
	$(RISDEVDIR)\riscom\lib\$(LIBDIR)\riscom.lib\
	$(RISDEVDIR)\risutl\lib\$(LIBDIR)\risuco.lib

EDIT_OBJECTS=\
	$(OBJDIR)\edt*.obj

!include $(RISDEVDIR)\risutl\build\maketplt.nt


editor: editor.exe
editor.exe: edtmain.obj $(EDIT_OBJECTS) $(EDIT_LIBS)
	@echo 	Linking with $(link) $(linkdebug) $(conflags)
	@$(link) $(linkdebug) $(conflags) -out:$@ $** $(conlibs)

edtmain.obj: edtmain.c
	@echo 	Compiling with $(cc) $(cdebug) $(cflags) $(cvars)
	@$(cc) -I..\internal -Iprt $(cdebug) $(cflags) $(cvars) $**
