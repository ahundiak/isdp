PDMINIT = $(PDM)/bin/PDMinit

APPOBJECTS=\
        $(PDM)/lib/PDMinit.a \
        $(PDM)/tools/PDMdbms_f.o \
        $(PDM)/tools/PDMpart_f.o \
        $(PDM)/tools/PDMstools1.o \
        $(PDM)/tools/PDMstools2.o \
        $(PDM)/ris/PDMrs_tool.o \
        $(LIBDIR)/libnfma.a \
        $(LIBDIR)/libnfmu.a \
        $(LIBDIR)/libnfmlists.a \
        $(LIBDIR)/libcmdtools.a \
        $(LIBDIR)/libnfmwf.a \
