        $(LIBDIR)/libnfmmem.a 

APPLIBRARIES=\
        -lcurses \
	-lbsd \
	-linc \
	-lnsl_s \
	-ltools_s \
	-lUMS \
	-lc_s \
	-lmalloc \
	-ldevi

OBJECTS = $(APPOBJECTS)

LIBRARIES = $(APPLIBRARIES)

image = $(LIBRARIES) $(OBJECTS)


image : 
	@echo ""
	acc -ansi -O3 -Dclipper $(TARGSTR) -o $(PDMINIT) \
	$(OBJECTS) \
	$(LIBRARIES)
	@echo ""
	chmod +s $(PDMINIT)
	strip $(PDMINIT)
	@ls -ls $(PDMINIT)
