        $(LIBDIR)/libnfmwf.a \
        $(LIBDIR)/libcmdtools.a \
        $(LIBDIR)/libdbtools.a \
        $(LIBDIR)/libnfmtools.a \
        $(LIBDIR)/libfilexfer.a \
        $(LIBDIR)/libnfmsql.a \
        $(LIBDIR)/libsystools.a \
        $(LIBDIR)/libnfmmem.a \
        $(LIBDIR)/libnfmnet.a \
        $(LIBDIR)/libtli.a \
        $(LIBDIR)/libnfmerr.a \
