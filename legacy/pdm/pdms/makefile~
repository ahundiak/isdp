.PRECIOUS:

.SUFFIXES: .a .c .o

LINKOPT =

SRC = .

IPATH =  \
	-I$(PDM)/include \
	-I$(PDM)/pdmapi/include \
	-I$(PDU)/include \
	-I$(NFM)/include \
	-I$(RIS)/include \
	-I$(RIS)/errmsg \
	-I$(FORMS)/include

CC = $(COMP)

COPT =  \
	$(COMP_OPT)

MOPT = -Dclipper=1 -D__clipper__=1

ocfiles = $(EXNUC)/bin/ocfiles


# Object files 

c_objects = \
	PDMattr.o \
	PDMcatalog.o \
	PDMflag.o \
	PDMgr_asly.o \
	PDMnlogin.o \
	PDMnet.o \
	PDMpart.o \
	PDMpartstr.o \
	PDMs_rtns.o \
	PDMsearch.o \
	PDMwf.o \
	PDMloc_part.o \
	PDMcat_class.o \
	PDMcat_attr.o \
	PDMattr_func.o \
	PDMupd_mem.o \
	PDMwhr_usd.o

objects = \
	$(c_objects) 


# main target

target: \
	PDMpdms.s


# src group targets

objects_TARGET = $(PDM)/lib/$(TARG)/PDMpdms.a

$(objects_TARGET): $(objects)
	@echo 
	ld $(LDOPT) -r -o $(PDM)/lib/$(TARG)/PDMpdms.a $(objects)
	@if test -n "$(VERBOSE_RULES)"; then echo "\tls -ls $(PDM)/lib/$(TARG)/PDMpdms.a"; fi; ls -ls $(PDM)/lib/$(TARG)/PDMpdms.a; echo

objects_FAST: FASTstart $(objects) FASTfinish $(objects_TARGET)

