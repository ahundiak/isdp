if (test $# = 0) 
	then
	echo "Need a time argument"
	exit
	fi

for arg in 1
do
	cd ..
	strip bin/PDMinit
	#strip bin/PDMcvt140
	#chmod 6775 bin/PDMinit
	#chmod 6775 bin/PDMcvt140

	touch $1 *
        find "bin/PDMinit" \
             "bin/schema.sh" \
		-print | sort | cpio -ov | \
		compress | fmu pdudev.root.rtpdev send - /usr2/ws_s/i_pdm$VERREF/pdmbin$TARG.p

	find config \
		-print | sort | cpio -ov | \
		compress | fmu pdudev.root.rtpdev send - /usr2/ws_s/i_pdm$VERREF/pdmconf$TARG.p
	find dbasupport \
		-print | sort | cpio -ov | \
		compress | fmu pdudev.root.rtpdev send - /usr2/ws_s/i_pdm$VERREF/pdmdbaspt.p
	find doc/signoff.doc \
             doc/signoff.wf \
		-print | sort | cpio -ov | \
		compress | fmu pdudev.root.rtpdev send - /usr2/ws_s/i_pdm$VERREF/pdmdoc.p

	find training -print | sort | cpio -ov | compress | \
	fmu pdudev.root.rtpdev send - /usr2/ws_s/i_pdm$VERREF/pdmtrain.p
done
