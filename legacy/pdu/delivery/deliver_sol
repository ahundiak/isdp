if (test $# = 0)
	then
	echo "Need a time argument"
	exit
	fi

for arg in 1
do
    cd ./pdusol
        ftp -n ${DELNODE} <<- END
                user root ${DEL_PSWD}
                binary
                put install.ssl ${WS_DIR}/i_pdu${VERREF}sol/install.ssl
                put remove.ssl ${WS_DIR}/i_pdu${VERREF}sol/remove.ssl
                put share.ssl ${WS_DIR}/i_pdu${VERREF}sol/share.ssl
                put product.def ${WS_DIR}/i_pdu${VERREF}sol/product.def
                put README ${WS_DIR}/i_pdu${VERREF}sol/README
                quit
		END
    cd ..
    dates > product.dates
done
