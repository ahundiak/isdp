if (test $# = 0)
	then
	echo "Need a time argument"
	exit
	fi

for arg in 1
do
    cd ../
	chmod 755 ppl/bin/*.e
	chmod 755 config/commands
	chmod 755 config/tables
	chmod 755 config/dload/init
	chmod 755 config/dload/cmds
	chmod 755 config/dload/prod
	chmod 755 config/dload/prod/pdu
	chmod 755 config/dload/prod/pdm
	chmod 755 config/english
	chmod 755 config/english/menus
	chmod 644 config/cmd_tbl_list
	chmod 644 config/language
	chmod 644 config/commands/*
	chmod 644 config/tables/*
	chmod 644 config/dload/init/*
        chmod 644 config/english/menus/*

    cd config/english/
	chmod 755 forms
	chmod 755 icons
	chmod 755 messages
	chmod 644 message_file
	chmod 644 man_pdu 
	chmod 644 forms/*
	chmod 644 messages/*
	chmod 644 HELPFILE
	chmod 644 HELPINFO
	chmod 644 icons/*
    cd ../../

	touch $1 `find ppl/bin/*.e -print`
	touch $1 config/*
	touch $1 config/dload/init/*
	touch $1 config/dload/prod/pdu/*
	touch $1 config/dload/prod/pdm/*
	touch $1 config/english/*
    cd config/english/
	touch $1 `find forms -print`
	touch $1 `find icons -print`
	touch $1 `find messages -print`

    cd ../../
	touch $1 `find config/commands -print`
	touch $1 `find config/tables -print`
	touch $1 `find training -print`

	find \
	ppl/bin/*.e \
	-print | cpio -ov | compress | \
	fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pdubin.p

	find \
	config/cmd_tbl_list	\
	config/commands \
	config/dload/init/* \
	config/dload/cmds/* \
	config/dload/prod/pdu/* \
	config/dload/prod/pdm/* \
	config/english/forms \
	config/english/icons \
	config/english/message_file \
	config/english/man_pdu \
	config/english/messages \
	config/language \
	config/tables \
	-print | sort | cpio -ov | compress | \
	fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pduconf.p

	find \
		config/english/HELPFILE \
		config/english/HELPINFO \
	-print | sort | cpio -ov | compress | \
	fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pduhelp.p

	find dbasupport \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pdudbaspt.p

	find assembly \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pduass.p

	find catalogs \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pducat.p

	find design \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pdudesign.p

	find filemgr \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pdufmgr.p

	find init \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pduinit.p

	find ldpdu \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pduldpdu.p

	find include \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pduinclude.p

	find lib \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pdulib.p

	find macros \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pdumacros.p

	find messages \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pdumess.p

	find misc \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pdumisc.p

	find pal \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pdupal.p

	find parts \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pduparts.p

	find pdmr \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pdupdmr.p

	find ppl \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pduppl.p

	find reports \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pdureports.p

	find spec \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pduspec.p

	find tools \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pdutools.p

	find delivery \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pdudel.p

	find design_doc \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pdudesdoc.p

	find kernal \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pdukernal.p

	find pdubuild \
		-print | sort | cpio -ov | \
		compress | fmu ${DELNODE}.root.${DEL_PSWD} send - ${WS_DIR}/i_pdu${VERREF}src/pdubuild.p

done
