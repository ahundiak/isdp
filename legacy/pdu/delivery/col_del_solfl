    cd $PDU/config/german

    find HELPINFO forms messages \
     -print | sort | cpio -oc | compress > pdu_gerint.ssl
     ftp -n pdmdel <<- END
         user root mdel
         binary 
         put pdu_gerint.ssl /usr3/ws_s/i_pdmflsol/pdu_gerint.ssl
         quit
	END
	rm pdu_gerint.ssl

    find HELPFILE \
     -print | sort | cpio -oc | compress > pdu_gerhlp.ssl
     ftp -n pdmdel <<- END
         user root mdel
         binary 
         put pdu_gerhlp.ssl /usr3/ws_s/i_pdmflsol/pdu_gerhlp.ssl
         quit
	END
	rm pdu_gerhlp.ssl

    cd $PDU/config/french
    find . \
     -print | sort | cpio -oc | compress > pdu_freint.ssl
     ftp -n pdmdel <<- END
         user root mdel
         binary 
         put pdu_freint.ssl /usr3/ws_s/i_pdmflsol/pdu_freint.ssl
         quit
	END
	rm pdu_freint.ssl

    cd $PDM/french
    find initial \
     -print | sort | cpio -oc | compress > pdm_frinit.ssl 
     ftp -n pdmdel <<- END
         user root mdel
         binary 
         put pdm_frinit.ssl /usr3/ws_s/i_pdmflsol/pdm_frinit.ssl
         quit
	END
	rm pdm_frinit.ssl

    cd $PDM/german
    find initial \
     -print | sort | cpio -oc | compress > pdm_grinit.ssl 
     ftp -n pdmdel <<- END
         user root mdel
         binary 
         put pdm_grinit.ssl /usr3/ws_s/i_pdmflsol/pdm_grinit.ssl
         quit
	END
	rm pdm_grinit.ssl

    cd $PDU/delivery/pdmfl_sol
    ftp -n pdmdel <<- END
         user root mdel
         binary 
         put install.ssl /usr3/ws_s/i_pdmflsol/install.ssl
         put remove.ssl /usr3/ws_s/i_pdmflsol/remove.ssl
         put README /usr3/ws_s/i_pdmflsol/README
         put product.def /usr3/ws_s/i_pdmflsol/product.def
         quit
	END
