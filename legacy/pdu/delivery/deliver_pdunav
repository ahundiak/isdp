if (test $# = 0)
	then
	echo "Need a time argument"
	exit
	fi

for arg in 1
do
    cd ./pdu
	touch $1 *
    chmod 774 Update.sh
    find Update.sh -print | cpio -ov | compress | \
		fmu pdmdel.root.newroot send - /usr/ws_s/i_pdunav/pduupdate.p
    fmu pdmdel.root.newroot \
		send install.sh /usr/ws_s/i_pdunav/install.sh
    fmu pdmdel.root.newroot \
		send share.sh /usr/ws_s/i_pdunav/share.sh
    fmu pdmdel.root.newroot \
		send README /usr/ws_s/i_pdunav/README
    fmu pdmdel.root.newroot \
		send remove.sh /usr/ws_s/i_pdunav/remove.sh
    fmu pdmdel.root.newroot \
		send product.def /usr/ws_s/i_pdunav/product.def
    cd ..
    dates > product.dates
done
