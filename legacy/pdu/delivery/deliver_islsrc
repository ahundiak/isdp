if (test $# = 0)
	then
	echo "Need a time argument"
	exit
	fi

for arg in 1
do
    cd ./pduislsrc
        ftp -n $DELNODE <<- END
                user root $DEL_PSWD
                binary
                put install.isl $WS_DIR/i_pduislsrc${VERREF}/install.isl
                put remove.isl $WS_DIR/i_pduislsrc${VERREF}/remove.isl
                put share.isl $WS_DIR/i_pduislsrc${VERREF}/share.isl
                put product.def $WS_DIR/i_pduislsrc${VERREF}/product.def
                put README $WS_DIR/i_pduislsrc${VERREF}/README
                quit
		END
    cd ..
    dates > product.dates
done
