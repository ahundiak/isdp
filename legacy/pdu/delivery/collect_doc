	cd $PDU/designdoc

	find . \
	-print | sort | cpio -ovc | compress | \
	fmu pdmdel.root.pdelrt send - /usr3/ws_s/i_designdoc/designdoc.p

    cd $PDU/delivery/designdoc
    fmu pdmdel.root.pdelrt \
                send install.sh /usr3/ws_s/i_designdoc/install.sh
    fmu pdmdel.root.pdelrt \
                send share.sh /usr3/ws_s/i_designdoc/share.sh
    fmu pdmdel.root.pdelrt \
                send README /usr3/ws_s/i_designdoc/README
    fmu pdmdel.root.pdelrt \
                send product.def /usr3/ws_s/i_designdoc/product.def

