chmod 777 cvtcmd;
cp ppl.sav ppl_table;
echo "Copying ppl_table to ppl.cmd";
cp ppl_table ppl.cmd;
echo "Convert ppl.cmd";
$PDU/config/tables/cvtcmd ppl.cmd $PDU/messages/PDUcommand.mb;
echo "Copying ppl.cm.new to ppl.cmp";
cp ppl.cmd.new ppl.cmd;
echo "Edit ppl.cmd";
#vi ppl.cmd;
sed -e 's/ PDUcommand.h/PDUcommand.h/g' ppl.cmd > ppl.out
mv ppl.out ppl.cmd
echo "Generate ppl.cmp";
/lib/cpp -P -I$PDU/include ppl.cmd | awk '{ if ($$0 != "") print $$0 }' > \
ppl.cmp;
echo "Copying ppl.cmp to ppl_table";
cp ppl.cmp ppl_table;
