OBJ=$(DBAPAR)/dba/objects
LIB=$(DBAPAR)/dba/lib


$(DBAPAR)/w_rundba/bin/dbasrv:   \
  $(OBJ)/srv_util.o \
  $(OBJ)/nfmcalls.o \
  $(OBJ)/r_exec.o \
  $(OBJ)/get_sqlc.o \
  $(OBJ)/dbnfm$(NFMVER).o \
  $(OBJ)/dbasrv.o
	$(CC) -o dbasrv $(TARG) \
  $(OBJ)/srv_util.o \
  $(OBJ)/nfmcalls.o \
  $(OBJ)/r_exec.o \
  $(OBJ)/get_sqlc.o \
  $(OBJ)/dbasrv.o \
  $(OBJ)/dbnfm$(NFMVER).o \
  $(NFMLIB) \
  /$(RIS)/ip32/ris/$(RISDP)/lib/$(C14)/ris.a \
  -lUMS -lxcmalloc \
  -linc -lnsl $(LIBDEVI) \
  -lmath -lxc -lix -lbsd -lc_s -ll -lPW
	mv dbasrv $(DBAPAR)/dbasrv/bin/dbasrv

