if [ "$LOGNAME" != "ingr" ] ; then echo "
	Sorry,!   LOGIN  AS   [1mINGR[0m   USER  TO  RUN  THIS  PROGRAMM ."
	tput bel
	echo ""
	exit 1
fi

tput clear
SRV=vdsdev
SRVLOG=devcert.devcert
export SRVDIR=ws_s/route

echo "SENDING RELEASE TO <$SRV.$SRVLOG> DIR <$SRVDIR>"



routefiles | fmu -av $SRV.$SRVLOG

echo "\n\t\t\t  [1m[7mProcess completed.[0m"
echo "\n"

