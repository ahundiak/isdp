if [ "$LOGNAME" != "route232" ] ; then echo "
	Sorry,!   LOGIN  AS   [1mroute232[0m   USER  TO  RUN  THIS  PROGRAMM ."
	tput bel
	echo ""
	exit 1
fi

tput clear
SRV=isdc_srv
SRVLOG=pub220
export SRVDIR=route

echo "SENDING RELEASE TO <$SRV.$SRVLOG> DIR <$SRVDIR>"


routefiles | fmu -av $SRV.$SRVLOG

echo "\n\t\t\t  [1m[7mProcess completed.[0m"
echo "\n"

