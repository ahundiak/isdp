# set -o ignoreeof
set -m
set -o vi

# PS1=`uname -n`": "'$PWD'" $ "
CURID=`id | cut -d'(' -f2 | cut -d')' -f1`
PS=`uname -n`
PS1A=`echo '\033[1m'`
PS1B=`echo '\033[0m'`
PS1='$PS1A($PS:$CURID)$PS1B $PWD
$ '


####
#       Backspace should be the erase character.  Make Delete the
#       abort.
####
/bin/stty erase ^H
#bin/stty intr 

# Setup development enviroment
. $HOME/isdp/build/build/isdp.env

