# Master env file for isdp development

export ISDP_SOURCE=$HOME/isdp
export ISDP_TARGET=$HOME/target
export ISDP_INSTALL=$HOME/ingr
export ISDP_LEGACY=$HOME/save/ingr

# This will be symbolically linked to ISDP_INSTALL
export ISDP_OPT_INGR=/opt/ingr

# This is hardcoded into some of the build scripts
export INGRHOME=/opt/ingr

export PATH=$PATH:$ISDP_OPT_INGR/bin
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$ISDP_OPT_INGR/lib
