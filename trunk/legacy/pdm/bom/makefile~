.PRECIOUS:

.SUFFIXES: .a .c .o

SRC = .

IPATH =  \
	-I$(PDU)/include \
	-I$(PDM)/include \
	-I$(RIS)/include \
	-I$(NFM)/include \
	-I$(DBA)/include

CC = acc -ansi -O3

MOPT = -Dclipper=1 -D__clipper__=1

ocfiles = $(EXNUC)/bin/ocfiles


# Object files 

c_objects = \
	PDMactive.o \
	PDMbom.o \
	PDMbomrtns.o \
	PDMreview.o \
	PDMbom_assy.o \
	PDMbom_attr.o \
	PDMgen_bom.o \
	PDMxsum_rps.o

objects = \
	$(c_objects) 


# src group targets

objects_TARGET = $(PDM)/lib/PDMbom.a

$(objects_TARGET): $(objects)
	@echo 
	ld $(LDOPT) -r -o $(PDM)/lib/PDMbom.a $(objects)
	@if test -n "$(VERBOSE_RULES)"; then echo "\tls -ls $(PDM)/lib/PDMbom.a"; fi; ls -ls $(PDM)/lib/PDMbom.a; echo

objects_FAST: FASTstart $(objects) FASTfinish $(objects_TARGET)

