.PRECIOUS:

.SUFFIXES: .a .c .o

SRC = .

IPATH =  \
	-I$(PDM)/include \
	-I$(PDU)/include \
	-I$(NFM)/include \
	-I$(RIS)/include \
	-I$(RIS)/errmsg

CC = $(COMP)

COPT =  \
	$(COMP_OPT)

MOPT = -Dclipper=1 -D__clipper__=1

ocfiles = $(EXNUC)/bin/ocfiles


# Object files 

c_objects = \
	PDMcopy_f.o \
	PDMffast.o \
	PDMcopy_m.o \
	PDMassy_mem.o \
	PDMfunc1.o \
	PDMcp_files.o \
	PDMfcopy_m.o \
	PDMiteminfo.o \
	PDMn_ci.o \
	PDMsettools.o \
	PDMn_co.o \
	PDMexp_copy.o \
	PDMsave_part.o

objects = \
	$(c_objects) 


# src group targets

objects_TARGET = $(PDM)/lib/PDMcicocp.a

$(objects_TARGET): $(objects)
	@echo 
	ld $(LDOPT) -r -o $(PDM)/lib/PDMcicocp.a $(objects)
	@if test -n "$(VERBOSE_RULES)"; then echo "\tls -ls $(PDM)/lib/PDMcicocp.a"; fi; ls -ls $(PDM)/lib/PDMcicocp.a; echo

objects_FAST: FASTstart $(objects) FASTfinish $(objects_TARGET)

