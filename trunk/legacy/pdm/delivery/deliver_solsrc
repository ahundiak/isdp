if (test $# = 0)
	then
	echo "Need a time argument"
	exit
	fi

for arg in 1
do
    cd ./pdmsolsrc
        ftp -n ${DELNODE} <<- END
                user root ${DEL_PSWD}
                binary
                put install.ssl ${WS_DIR}/i_pdmsolsrc${VERREF}/install.ssl
                put remove.ssl ${WS_DIR}/i_pdmsolsrc${VERREF}/remove.ssl
                put share.ssl ${WS_DIR}/i_pdmsolsrc${VERREF}/share.ssl
                put product.def ${WS_DIR}/i_pdmsolsrc${VERREF}/product.def
                put README ${WS_DIR}/i_pdmsolsrc${VERREF}/README
                quit
		END
    cd ..
    dates > product.dates
done
