if (test $# = 0) 
	then
	echo "Need a time argument"
	exit
	fi

for arg in 1
do
    cd ./pdm
	touch $1 *
    fmu ${DELNODE}.root.${DEL_PSWD} \
		send install.sh ${WS_DIR}/i_pdm${VERREF}/install.sh
    fmu ${DELNODE}.root.${DEL_PSWD} \
		send README ${WS_DIR}/i_pdm${VERREF}/README
    fmu ${DELNODE}.root.${DEL_PSWD} \
		send remove.sh ${WS_DIR}/i_pdm${VERREF}/remove.sh
    fmu ${DELNODE}.root.${DEL_PSWD} \
		send product.def ${WS_DIR}/i_pdm${VERREF}/product.def
    cd ..
done
