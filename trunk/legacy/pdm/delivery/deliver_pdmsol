if (test $# = 0)
	then
	echo "Need a time argument"
	exit
	fi

for arg in 1
do
    cd ./pdmsol
        ftp -n pdudev <<- END
                user root udev
                binary
                put install.ssl ${WS_DIR}/i_pdm${VERREF}sol/install.ssl
                put remove.ssl ${WS_DIR}/i_pdm${VERREF}sol/remove.ssl
                put product.def ${WS_DIR}/i_pdm${VERREF}sol/product.def
                put README ${WS_DIR}/i_pdm${VERREF}sol/README
                quit
		END
    cd ..
    dates > product.dates
done
