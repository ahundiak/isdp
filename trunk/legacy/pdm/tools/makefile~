.PRECIOUS:

.SUFFIXES: .a .c .o

SRC = .

IPATH =  \
	-I$(PDU)/include \
	-I$(NFM)/include \
	-I$(EXNUC)/include

CC = acc -ansi -O3 

MOPT = -Dclipper=1 -D__clipper__=1

ocfiles = $(EXNUC)/bin/ocfiles


# Object files 

c_objects = \
	PDMrassy.o \
	PDMrattr.o \
	PDMrbom.o \
	PDMrcanchko.o \
	PDMrcatalog.o \
	PDMrchkin.o \
	PDMrchkout.o \
	PDMrcopy.o \
	PDMrdebug.o \
	PDMrdetach.o \
	PDMrdyn_part.o \
	PDMrflag.o \
	PDMrfmgr.o \
	PDMrmisc.o \
	PDMrnfm_rpts.o \
	PDMrpart.o \
	PDMrpart_p.o \
	PDMrpartstr.o \
	PDMrpdm_rpts.o \
	PDMrrps_f.o \
	PDMrsearch.o \
	PDMrtools.o \
	PDMruser.o \
	PDMrwf.o \
	PDUstatus.o \
	PDMrcriteria.o \
	PDMrexec.o

objects = \
	$(c_objects) 


# src group targets

objects_TARGET = $(PDU)/lib/pdmr.a

$(objects_TARGET): $(objects)
	@echo 
	ld $(LDOPT) -r -o $(PDU)/lib/pdmr.a $(objects)
	@if test -n "$(VERBOSE_RULES)"; then echo "\tls -ls $(PDU)/lib/pdmr.a"; fi; ls -ls $(PDU)/lib/pdmr.a; echo

objects_FAST: FASTstart $(objects) FASTfinish $(objects_TARGET)

