# Do not alter this SPC information: $Revision: 1.3.52.1 $
!include $(RISDEVDIR)\riscom\build\defines.nt

IPATH=-I$(RISDEVDIR)\riscom\internal

fast: $(BINDIR)\ristcpsr.exe

# Update the resource ristcpsr.rbj
$(BINDIR)\ristcpsr.res: ristcpsr.rcv $(RISDEVDIR)\riscom\internal\risver.h 
	$(rc) $(rcvars) -r $(IPATH) -fo $(BINDIR)\ristcpsr.res $(cvars) ristcpsr.rcv

$(BINDIR)\ristcpsr.exe: $(RISTCPSR_OBJS) $(RISTCPSR_ARCHIVES) $(BINDIR)\ristcpsr.res
	@echo.
	@echo RISTCPSR_OBJS RISTCPSR_ARCHIVES   -to-   $@
	@echo 	Linking with $(link) $(linkdebug) $(conflags)
	$(link) $(linkdebug) $(conflags) -out:$(@) $(RISTCPSR_OBJS) $(RISTCPSR_ARCHIVES) $(RISTCPSR_LIBS) $(BINDIR)\ristcpsr.res $(conlibsdll)

strip:

links:
	@echo 	$(BINDIR)\ristcpsr.exe   -to-   ristcpsr.exe
	@copy $(BINDIR)\ristcpsr.exe ristcpsr.exe

rmlinks:
	@echo 	Removing ristcpsr.exe
	@-del ristcpsr.exe 2>NUL
