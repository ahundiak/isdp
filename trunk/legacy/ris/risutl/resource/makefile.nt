!include $(RISDEVDIR)\ristools\include\makeenv.nt

IPATH= -i $(RISDEVDIR)\risutl\config\english\icons\
	-i $(RISDEVDIR)\riscom\internal

IPATH_RLICLIST= -i $(RISDEVDIR)\risutl\config\english\icons\
	-i $(RISDEVDIR)\riscom\internal \
        -i $(RISDEVDIR)\risutl\rliclist

all: version objects rliclist_obj objects32s rliclist_obj32s

version:
	@echo.
	@echo Making $(RISDEVDIR)\riscom\internal\risver.h
	@cd $(RISDEVDIR)\riscom\build
	@makevers
	@cd $(RISDEVDIR)\risutl\resource

objects: \
	$(OBJDIR)\risbatch.res \
	$(OBJDIR)\risclnsr.res \
	$(OBJDIR)\risdcode.res \
	$(OBJDIR)\risdtype.res \
	$(OBJDIR)\risgui.res \
	$(OBJDIR)\rislod.res \
	$(OBJDIR)\rismgr.res \
	$(OBJDIR)\risplbck.res \
	$(OBJDIR)\risrecrd.res \
	$(OBJDIR)\risrun.res \
	$(OBJDIR)\risunlod.res \
	$(OBJDIR)\risuparm.res \
	$(OBJDIR)\formsdll.res \
	$(OBJDIR)\ldulddll.res 

$(OBJDIR)\risbatch.res: $(RISDEVDIR)\riscom\internal\risver.h
$(OBJDIR)\risrun.res: $(RISDEVDIR)\riscom\internal\risver.h
$(OBJDIR)\risclnsr.res: $(RISDEVDIR)\riscom\internal\risver.h $(RISDEVDIR)\risutl\config\english\icons\risclnsr.ico
$(OBJDIR)\risdcode.res: $(RISDEVDIR)\riscom\internal\risver.h $(RISDEVDIR)\risutl\config\english\icons\risdcode.ico
$(OBJDIR)\risdtype.res: $(RISDEVDIR)\riscom\internal\risver.h $(RISDEVDIR)\risutl\config\english\icons\risdtype.ico
$(OBJDIR)\risgui.res: $(RISDEVDIR)\riscom\internal\risver.h $(RISDEVDIR)\risutl\config\english\icons\risgui.ico
$(OBJDIR)\rislod.res: $(RISDEVDIR)\riscom\internal\risver.h $(RISDEVDIR)\risutl\config\english\icons\rislod.ico
$(OBJDIR)\rismgr.res: $(RISDEVDIR)\riscom\internal\risver.h $(RISDEVDIR)\risutl\config\english\icons\rismgr.ico
$(OBJDIR)\risplbck.res: $(RISDEVDIR)\riscom\internal\risver.h $(RISDEVDIR)\risutl\config\english\icons\risplbck.ico
$(OBJDIR)\risrecrd.res: $(RISDEVDIR)\riscom\internal\risver.h $(RISDEVDIR)\risutl\config\english\icons\risrecrd.ico
$(OBJDIR)\risunlod.res: $(RISDEVDIR)\riscom\internal\risver.h $(RISDEVDIR)\risutl\config\english\icons\risunlod.ico
$(OBJDIR)\risuparm.res: $(RISDEVDIR)\riscom\internal\risver.h $(RISDEVDIR)\risutl\config\english\icons\risuparm.ico
$(OBJDIR)\formsdll.res: $(RISDEVDIR)\riscom\internal\risver.h
$(OBJDIR)\ldulddll.res: $(RISDEVDIR)\riscom\internal\risver.h
.rc{$(OBJDIR)\}.res:
	@echo.
	@echo.  $(@B).rc  -to-  $@
	@echo   Calling RC
	@rc $(IPATH) $(RCFLAGS) -r -DWIN32 -FO $(OBJDIR)\$(@B).res $(@B).rc

# 1996.05.20 - Melania M Stewart - rliclist has a an extra include path
rliclist_obj: \
        $(RISDEVDIR)\riscom\internal\risver.h $(RISDEVDIR)\risutl\rliclist\rliclist.h rliclist.rc
	@echo.
	@echo.  $(@B).rc  -to-  $@
	@echo   Calling RC
	@rc $(IPATH_RLICLIST) $(RCFLAGS) -r -DWIN32 -FO $(OBJDIR)\rliclist.res rliclist.rc
        @cvtres -$(CPU) $(OBJDIR)\rliclist.res -o $(OBJDIR)\rliclist.rbj

