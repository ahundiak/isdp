#ifndef _SMM_DEF
#define _SMM_DEF

/* Block type flags */

#define SMM_DYNAMIC			1

typedef struct smm_generic_list
  {
    void *addr;
    void *source;
    void *other;
    unsigned long size;
    struct smm_generic_list *next, *prev;
  } smm_generic_list;

typedef struct smm_blocktype
  {
    void *start, *end;
    unsigned long size;
    unsigned long maxsize;
    unsigned long freesize;
    unsigned long flags;
    smm_generic_list *freelist;
    smm_generic_list *usedlist;
  } smm_blocktype;

typedef struct gennodetype
  {
    void *ptr;
    struct gennodetype *next;
  } gennodetype;

typedef struct
  { 
    long size;
    gennodetype *top;
  } genstacktype;   

typedef struct dba_avl_tree_node
  {
    void *data;
    struct dba_avl_tree_node *left;
    struct dba_avl_tree_node *right;
    short bal;
  } dba_avl_tree_node;

typedef struct dba_symtree_desc_node
  {
    unsigned long name;
    unsigned long operand;
  } dba_symtree_desc_node;
#endif
