#define FALSE 0
#define TRUE  1

extern PDUppl_checkout_part();
extern _pdm_debug();

main()
{

int	return_status;

pdm$checkout_part(	status 		= &return_status,
			catalog_name	= "standard_cat",
			partid		= "macropart1",
