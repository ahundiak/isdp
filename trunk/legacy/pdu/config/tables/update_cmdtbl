chmod 777 cvtcmd;
echo "Copying command_table to command.sav";
cp command_table command.sav;
echo "Copying command_table to command.cmp";
cp command_table command.cmd;
echo "Convert command.cmd";
$PDU/config/tables/cvtcmd command.cmd $PDU/messages/PDUcommand.mb;
echo "Copying command.cm.new to command.cmp";
cp command.cm.new command.cmd;
echo "Edit command.cmd";
#vi command.cmd;
sed -e 's/ PDUcommand.h/PDUcommand.h/g' command.cmd >command.out
mv command.out command.cmd;
echo "Generate command.cmp";
/lib/cpp -P -I$PDU/include command.cmd | awk '{ if ($$0 != "") print $$0 }' > \
command.cmp;
echo "Copying command.cmp to command_table";
cp command.cmp command_table;
