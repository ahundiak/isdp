# main target

target: \
	PDUdesign.s


# src group targets

objects_TARGET = $(PDU)/lib/$(TARG)/PDUdesign.a

$(objects_TARGET): $(objects)
	@echo 
	ld $(LDOPT) -r -o $(PDU)/lib/$(TARG)/PDUdesign.a $(objects)
	@if test -n "$(VERBOSE_RULES)"; then echo "\tls -ls $(PDU)/lib/$(TARG)/PDUdesign.a"; fi; ls -ls $(PDU)/lib/$(TARG)/PDUdesign.a; echo

objects_FAST: FASTstart $(objects) FASTfinish $(objects_TARGET)

