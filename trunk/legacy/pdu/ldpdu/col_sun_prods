echo
echo "collecting PDUDP"
echo

./collect_sundp $time

echo
echo "delivering PDUDP"
echo

./deliver_sundp $time

echo
echo "collecting PDUSRC"
echo

./collect_sunsrc $time

echo
echo "delivering PDUSRC"
echo

./deliver_sunsrc $time

echo
echo "collecting PDMSRC"
echo

cd $PDM/delivery
