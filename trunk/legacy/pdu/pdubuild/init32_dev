#	$1 :	The version reference number
#		Ex. Version 2.4.x.x would be referenced with 24.

cd /usr2/load32
echo "Initialize PDU $1"
/usr/ip32/deltools/node/Initial /usr2/ws_s/i_pdu$1
echo "Initialize PDU $1"
/usr/ip32/deltools/node/Initial /usr2/ws_s/i_pdu$1sol
echo "Initialize PDU $1"
/usr/ip32/deltools/node/Initial /usr2/ws_s/i_pdu$1sgi
echo "Initialize PDM $1"
/usr/ip32/deltools/node/Initial /usr2/ws_s/i_pdm$1
echo "Initialize PDUDP $1"
/usr/ip32/deltools/node/Initial /usr2/ws_s/i_pdu$1dp
echo "Initialize PDMNORM $1"
/usr/ip32/deltools/node/Initial /usr2/ws_s/i_pdmnorm

cd /usr2/32src
echo
echo "Initialize PDU $1 Source for CLIPPER"
/usr/ip32/deltools/node/Initial /usr2/ws_s/i_pdu$1src
echo
echo "Initialize PDU $1 Source for SPARC"
/usr/ip32/deltools/node/Initial /usr2/ws_s/i_pdusolsrc$1
echo
echo "Initialize PDM $1 Source for CLIPPER"
/usr/ip32/deltools/node/Initial /usr2/ws_s/i_pdm$1src
echo
echo "Initialize PDM $1 Source for SPARC"
/usr/ip32/deltools/node/Initial /usr2/ws_s/i_pdmsolsrc$1
echo
