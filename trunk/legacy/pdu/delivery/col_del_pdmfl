    cd $PDU/config/german

    find HELPINFO forms messages \
     -print | sort | cpio -ovc | compress | \
     fmu pdmdel.root.mdel send - /usr3/ws_s/i_pdmfl/pdu_gerint.p

    find HELPFILE \
     -print | sort | cpio -ovc | compress | \
     fmu pdmdel.root.mdel send - /usr3/ws_s/i_pdmfl/pdu_gerhelp.p

    cd $PDU/config/french
    find . \
     -print | sort | cpio -ovc | compress | \
     fmu pdmdel.root.mdel send - /usr3/ws_s/i_pdmfl/pdu_freint.p

    cd $PDM/french
    find initial \
     -print | sort | cpio -ovc | compress | \
     fmu pdmdel.root.mdel send - /usr3/ws_s/i_pdmfl/pdm_freinit.p

    cd $PDM/german
    find initial \
     -print | sort | cpio -ovc | compress | \
     fmu pdmdel.root.mdel send - /usr3/ws_s/i_pdmfl/pdm_gerinit.p

    cd $PDU/delivery/pdmfl
    fmu pdmdel.root.mdel \
                send install.sh /usr3/ws_s/i_pdmfl/install.sh
    fmu pdmdel.root.mdel \
                send remove.sh /usr3/ws_s/i_pdmfl/remove.sh
    fmu pdmdel.root.mdel \
                send README /usr3/ws_s/i_pdmfl/README
    fmu pdmdel.root.mdel \
                send product.def /usr3/ws_s/i_pdmfl/product.def

