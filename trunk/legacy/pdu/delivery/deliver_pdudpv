if (test $# = 0)
	then
	echo "Need a time argument"
	exit
	fi

for arg in 1
do
    cd ./pdudp
	touch $1 *
    chmod 774 Update.sh
    find Update.sh -print | cpio -ov | compress | \
		fmu pdudev.root.rtpdev send - /usr/ws_s/i_pdu${VERREF}dp/pduupdate.p
    fmu pdudev.root.rtpdev \
		send install.sh /usr2/ws_s/i_pdu${VERREF}dp/install.sh
    fmu pdudev.root.rtpdev \
		send share.sh /usr2/ws_s/i_pdu${VERREF}dp/share.sh
    fmu pdudev.root.rtpdev \
		send README /usr2/ws_s/i_pdu${VERREF}dp/README
    fmu pdudev.root.rtpdev \
		send remove.sh /usr2/ws_s/i_pdu${VERREF}dp/remove.sh
    fmu pdudev.root.rtpdev \
		send product.def /usr2/ws_s/i_pdu${VERREF}dp/product.def
    cd ..
    dates > product.dates
done
