		-print | sort | cpio -ovc | compress > pduass.ssl
	ftp -n ${DELNODE} <<- END
                user root ${DEL_PSWD}
                binary
                put pduass.ssl ${WS_DIR}/i_pdusolsrc${VERREF}/pduass.ssl
                quit
		END
        rm pduass.ssl
