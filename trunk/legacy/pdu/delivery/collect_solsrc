	ftp -n ${DELNODE} <<- END
                user root ${DEL_PSWD}
                binary
                put pduhelp.ssl ${WS_DIR}/i_pdusolsrc${VERREF}/pduhelp.ssl
                quit
		END
        rm pduhelp.ssl
        echo

	find dbasupport \
		-print | sort | cpio -ovc | compress > pdudbaspt.ssl
	ftp -n ${DELNODE} <<- END
                user root ${DEL_PSWD}
                binary
                put pdudbaspt.ssl ${WS_DIR}/i_pdusolsrc${VERREF}/pdudbaspt.ssl
                quit
		END
        rm pdudbaspt.ssl
