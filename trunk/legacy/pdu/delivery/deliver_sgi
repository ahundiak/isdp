if (test $# = 0)
	then
	echo "Need a time argument"
	exit
	fi

for arg in 1
do
    cd ./pdusgi
        ftp -n ${DELNODE} <<- END
                user root ${DEL_PSWD}
                binary
                put install.sgi ${WS_DIR}/i_pdu${VERREF}sgi/install.sgi
                put remove.sgi ${WS_DIR}/i_pdu${VERREF}sgi/remove.sgi
                put share.sgi ${WS_DIR}/i_pdu${VERREF}sgi/share.sgi
                put product.def ${WS_DIR}/i_pdu${VERREF}sgi/product.def
                put README ${WS_DIR}/i_pdu${VERREF}sgi/README
                quit
		END
    cd ..
    dates > product.dates
done
