.PRECIOUS:

.SUFFIXES: .a .c .o

SRC = .

IPATH =  \
	-I$(PDU)/include \
	-I$(PDM)/include \
	-I$(EXNUC)/include \
	-I$(GRNUC)/include \
	-I$(NFM)/include \
	-I$(DBA)/include

CC = acc -ansi -O3 -Atarg=c400

MOPT = -Dclipper=1 -D__clipper__=1

ocfiles = $(EXNUC)/bin/ocfiles


# Object files 

c_objects = \
	PDUverify.o \
	PDUbfr_tools.o \
	PDUform.o \
	PDUupdat_buf.o \
	PDUdebug.o \
	PDUstr_attr.o \
	PDUvalid.o \
	PDUlinklist.o \
	PDUchsym_frm.o

objects = \
	$(c_objects) 


# src group targets

objects_TARGET = $(PDU)/lib/PDUtools.a

$(objects_TARGET): $(objects)
	@echo 
	ld $(LDOPT) -r -o $(PDU)/lib/PDUtools.a $(objects)
	@if test -n "$(VERBOSE_RULES)"; then echo "\tls -ls $(PDU)/lib/PDUtools.a"; fi; ls -ls $(PDU)/lib/PDUtools.a; echo

objects_FAST: FASTstart $(objects) FASTfinish $(objects_TARGET)

