<root>
<om_classes>
  <om_class name="Root" extends="Root" version="3:0">
    <instance_data>
      <attr type="OM_S_OBJECTHDR" name="OPPobjecthdr" />
    </instance_data>
    <messages>
      <message name="debug" />
      <message name="delete">
        <attr type="int" name="f_defer_flag" />
      </message>
      <message name="connect">
        <param type="OM_S_CHANSELECT" name="to_sel"   />
        <param type="int"             name="to_idx"   />
        <param type="OM_S_OBJID"      name="fr_objid" />
		    <param type="OMuword"         name="fr_os"    />
		    <param type="OM_S_CHANSELECT" name="fr_sel"   />
		    <param type="int"             name="fr_idx"   />
      </message>
      <message name="disconnect" >
        <param type="OM_S_CHANSELECT" name="to_sel"   />
        <param type="OM_S_OBJID"      name="fr_objid" />
        <param type="OMuword"         name="fr_os"    />
        <param type="OM_S_CHANSELECT" name="fr_sel"   />
      </message>

      <message name="wild_disconnect" >
        <param type="OM_S_CHANSELECT" name="to_sel" />
      </message>

      <message name="move_chan" >
        <param type="OM_S_CHANSELECT" name="to_sel"   />
        <param type="OM_S_OBJID"      name="fr_objid" />
        <param type="OMuword"         name="fr_os"    />
        <param type="OM_S_CHANSELECT" name="fr_sel"   />
      </message>

      <message name="copy_chan" >
		    <param type="OM_S_CHANSELECT" name="to_sel"    />
		    <param type="OM_S_OBJID"      name="fr_objid"  />
		    <param type="OMuword"         name="fr_os"     />
		    <param type="OM_S_CHANSELECT" name="fr_sel"    />)
      </message>

      <message name="unknown" >
        <param type="OM_S_MESSAGE *" name="msg" />
      </message>

      <message name="range_disconnect" >
        <param type="OM_S_CHANSELECT" name="to_sel"   />
        <param type="int"             name="low_idx"  />
        <param type="int"             name="hi_idx"   />
      </message>

      <message name="set_operation" >
        <param type="OM_S_CHANSELECT" name="to_sel"    />
        <param type="OM_S_OBJID"      name="op2_objid" />
        <param type="OMuword"         name="op2_os"    />
        <param type="OM_S_CHANSELECT" name="op2_sel"   />
        <param type="OM_S_OBJID"      name="op3_objid" />
        <param type="OMuword"         name="op3_os"    />
        <param type="OM_S_CHANSELECT" name="op3_sel"   />
        <param type="char"            name="operation" />
        <param type="OMuword"         name="preserve)" />
      </message>

      <message name="ARwrite">
        <param type="int *" name="return_code" />
        <param type="int"   name="starting"    />
      </message>

      <message name="ARread" >
        <param type="int *"  name="return_code" />
        <param type="char *" name="keyword"    />
      </message>

    </messages>
  </om_class>
</om_classes>
</root>
