
#include "VDppl.h"
#include "VDobj.h"
#include "v_slcdef.h"
#include "v_pplslc.h"

extern VDstrupr(), printf();

main()
{
IGRchar			*p, wat_name[128];
struct GRobj_env	wat_line, planeOE;

   *wat_name = 0;
   vd$ppl_getarg_at_index( which_info 	= INPUT_INFO,
			   index	= 0,
			   value	= &wat_line );

   vdobj$GetTemplate( 	objOE 	   = &wat_line,
			nth	   = 2,	
			templateOE = &planeOE    );

   vdobj$Get  (objOE = &planeOE, objName = wat_name );
   VDstrupr(wat_name);

   p = strchr( wat_name, '_' );
   if( p ) strcpy( wat_name, p+1 );

   vd$ppl_set_size( which_info 	= OUTPUT_INFO,
		    size	= 1 );

   vd$ppl_setarg_at_index( which_info  	= OUTPUT_INFO,
			   index	= 0,
			   type		= VD_string,
			   value	= wat_name  );

wrapup:
   return 1;
}
