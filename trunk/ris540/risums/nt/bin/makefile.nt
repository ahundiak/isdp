!include $(RISDEVDIR)\ristools\include\makeenv.nt
!include $(RISDEVDIR)\risums\nt\build\defines.nt

fast: clean $(BINDIR)\risumscc.exe

$(BINDIR)\risumscc.exe: $(RISUMS_COMP_OBJS) $(RISUMS_RESOURCES) $(RISUMS_LIB)
	@echo.
	@echo 	$(RISUMS_COMP_OBJS) $(RISUMS_RESOURCES) $(RISUMS_LIB) -to- $@
	@echo.
	@$(link) $(linkdebug) $(conflags) -out:$(@) @<<
$(RISUMS_COMP_OBJS)
$(RISUMS_RESOURCES)
$(RISUMS_LIB)
$(conlibsdll)
<<
	@echo.

clean:
	@echo "deleting executable in $(BINDIR) directory"
	@-del $(BINDIR)\risumscc.exe
