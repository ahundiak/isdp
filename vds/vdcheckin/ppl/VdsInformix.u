#include "ciminimum.h"
#include "cievent.h"
#include "stdio.h"
#include "VDSris_def.h"
#include "PDUerror.h"

extern	int	VDSverify_login();
extern	char    VDSenvir_name[];
extern  int	VDterm();       /* vdcheckin/func/VDterm.C */

main ()
{
	char	ServerName[100];

	message("Access Database");

	if( VDSverify_login() != PDM_S_SUCCESS ){

		status("User not logged in to Database");
		return FAILURE;
	}

	/* Retrieve PDU server name */
	strcpy ( ServerName, VDSenvir_name);

	VDterm( ServerName );
}
